{
  "name": "ngx-bootstrap-dialog-demo",
  "description": "Simple and customizable dialogs for Angular built on top of NG Bootstrap",
  "version": "0.0.0-development",
  "private": false,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run dev & ng serve -o",
    "publish:demo": "ng deploy --base-href=/ngx-bootstrap-dialog/",
    "publish:demo:ci": "ng deploy --base-href=/ngx-bootstrap-dialog/ --repo=https://github.com/manzapanza/ngx-bootstrap-dialog.git --name=manzapanza --email=maxappbr@gmail.com",
    "publish:lib": "cd ./dist/ngx-bootstrap-dialog && npx semantic-release",
    "build:lib": "ng build ngx-bootstrap-dialog",
    "test:lib": "npm run dev & ng test ngx-bootstrap-dialog",
    "test:lib:ci": "ng test ngx-bootstrap-dialog -- --no-watch --no-progress --browsers=ChromeHeadlessCI --code-coverage",
    "lint:lib": "ng lint ngx-bootstrap-dialog",
    "e2e:lib": "ng e2e ngx-bootstrap-dialog",
    "coverage:lib": "cat ./coverage/ngx-bootstrap-dialog/lcov.info | npx coveralls",
    "sync": "npm run sync:readme && npm run sync:version",
    "sync:readme": "copyfiles ./README.md ./projects/ngx-bootstrap-dialog/",
    "sync:version": "rjp ./projects/ngx-bootstrap-dialog/package.json version $npm_package_version",
    "dev": "npm run dev:env && npm run sync && npm run dev:change",
    "dev:env": "ngx-scripts env npm_package_version",
    "dev:change": "onchange './README.md' './package.json' -- npm run sync",
    "postinstall": "ngcc"
  },
  "keywords": [
    "angular",
    "ngx",
    "bootstrap",
    "dialog",
    "ng-bootstrap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manzapanza/ngx-bootstrap-dialog.git"
  },
  "bugs": {
    "url": "https://github.com/manzapanza/ngx-bootstrap-dialog/issues"
  },
  "homepage": "https://github.com/manzapanza/ngx-bootstrap-dialog#readme",
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/localize": "^9.1.12",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "bootstrap": "^4.4.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.3.0",
    "coveralls": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "onchange": "^7.0.2",
    "protractor": "~7.0.0",
    "replace-json-property": "^1.4.3",
    "semantic-release": "^17.1.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}